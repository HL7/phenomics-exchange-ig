		</parameter>
		<parameter>
			<code value="copyrightyear" />
			<value value="2020+" />
		</parameter>
		<parameter>
			<code value="path-resource" />
			<value value="input/profiles" />
		</parameter>


		<parameter>
			<code value="logging" />
			<value value="init" />
		</parameter>
		<parameter>
			<code value="logging" />
			<value value="progress" />
		</parameter>
		<parameter>
			<code value="logging" />
			<value value="tx" />
