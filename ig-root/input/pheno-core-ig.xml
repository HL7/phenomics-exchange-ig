<?xml version="1.0" encoding="UTF-8"?>
<ImplementationGuide xmlns="http://hl7.org/fhir"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://hl7.org/fhir http://hl7.org/fhir/R4/fhir-single.xsd">
	<id value="pheno-core-ig" />
	<url
		value="http://github.com/phenopackets/core-ig/ImplementationGuide/pheno-core-ig" />
	<version value="0.0.1" />
	<name value="PhenopacketsCoreIg" />
	<title value="Phenopackets Core IG" />
