	<definition>
		<grouping>
			<name value="base-profiles" />
			<description value="Base profiles" />
		</grouping>
		<resource>
			<reference>
				<reference value="CodeSystem/hpo" />
			</reference>
		</resource>
		<resource>
			<reference>
				<reference value="CodeSystem/karyotypic-sex" />
			</reference>
		</resource>
		<resource>
			<reference>
				<reference value="ValueSet/karyotypic-sex" />
			</reference>
		</resource>
		<resource>
			<reference>
				<reference value="StructureDefinition/karyotypic-sex" />
			</reference>
		</resource>

		<resource>
			<reference>
				<reference value="ValueSet/observation-phenotypic-abnormality" />
			</reference>
		</resource>
		<resource>
			<reference>
				<reference value="ValueSet/phenotypic-modifier" />
			</reference>
		</resource>
		<resource>
			<reference>
				<reference value="ValueSet/phenotypic-severity" />
			</reference>
		</resource>
		<resource>
			<reference>
				<reference value="ValueSet/phenotypic-onset" />
			</reference>
		</resource>
		<resource>
			<reference>
				<reference value="ValueSet/phenotypic-component" />
			</reference>
		</resource>
		<resource>
			<reference>
				<reference
					value="StructureDefinition/observation-phenotypic-abnormality" />
			</reference>
		</resource>

		<!-- Categories vocab -->

		<resource>
			<reference>
				<reference value="CodeSystem/categories" />
			</reference>
		</resource>
		<resource>
			<reference>
				<reference value="ValueSet/observation-categories" />
			</reference>
		</resource>

		<!-- Extensions -->
		<resource>
			<reference>
				<reference
					value="StructureDefinition/ext-validation-error-count" />
			</reference>
		</resource>

		<!-- Examples -->
		<resource>
			<reference>
				<reference
					value="Observation/phenotypic-abnormality-valid-1" />
			</reference>
			<name
				value="A valid phenotypic abnormality Observation example." />
			<exampleCanonical
				value="http://github.com/phenopackets/core-ig/StructureDefinition/observation-phenotypic-abnormality" />
		</resource>
		<resource>
			<reference>
				<reference
					value="Observation/phenotypic-abnormality-invalid-1" />
			</reference>
			<name
				value="An INVALID phenotypic abnormality Observation example." />
			<exampleCanonical
				value="http://github.com/phenopackets/core-ig/StructureDefinition/observation-phenotypic-abnormality" />
		</resource>

		<resource>
			<reference>
				<reference value="Observation/karyotypic-sex-invalid-1" />
			</reference>
			<name value="An INVALID karyotypic sex Observation instance." />
			<exampleCanonical
				value="http://github.com/phenopackets/core-ig/StructureDefinition/karyotypic-sex" />
		</resource>
		<resource>
			<reference>
				<reference value="Observation/karyotypic-sex-valid-1" />
			</reference>
			<name value="A VALID karyotypic sex Observation instance." />
			<exampleCanonical
				value="http://github.com/phenopackets/core-ig/StructureDefinition/karyotypic-sex" />
		</resource>

		<resource>
			<reference>
				<reference value="ValueSet/test" />
			</reference>
		</resource>


		<!-- Search parameters -->

		<resource>
			<reference>
				<reference
					value="SearchParameter/service-request-supporting-info" />
			</reference>
		</resource>



		<!-- Base resources to have a placeholder page in the IG -->

		<!-- <resource> <reference> <reference value="StructureDefinition/Patient" 
			/> </reference> <name value="Base Patient profile" /> <description value="Base 
			Patient profile description" /> <groupingId value="base-profiles" /> </resource> 
			<resource> <reference> <reference value="StructureDefinition/ServiceRequest" 
			/> </reference> <name value="Base ServiceRequest profile" /> <description 
			value="Base ServiceRequest profile description" /> </resource> <resource> 
			<reference> <reference value="StructureDefinition/Bundle" /> </reference> 
			<name value="Base Bundle profile" /> <description value="Base Bundle profile 
			description" /> </resource> <resource> <reference> <reference value="StructureDefinition/Person" 
			/> </reference> <name value="Base Person profile" /> <description value="Base 
			Person profile description" /> </resource> -->
		<page>
			<!-- The root will always be toc.html - the template will force it if 
				you don't do it -->
			<nameUrl value="toc.html" />
			<title value="Table of Contents" />
			<generation value="html" />
			<page>
				<nameUrl value="index.html" />
				<title value="Phenopackets Core IG Home Page" />
				<generation value="html" />
			</page>
			<page>
				<nameUrl value="background.html" />
				<title value="Background" />
				<generation value="html" />
			</page>
		</page>
		<!-- <parameter> <code value="path-resource" /> <value value="input/base-spec" 
			/> </parameter> -->
		<!-- copyright year is a mandatory parameter -->
		<parameter>
			<code value="copyrightyear" />
			<value value="2020+" />
		</parameter>
		<!-- releaselabel should be the ballot status for HL7-published IGs. -->
		<parameter>
			<code value="releaselabel" />
			<value value="GitHub Travis Build" />
		</parameter>
		<parameter>
			<code value="logging" />
			<value value="init" />
		</parameter>
	</definition>
</ImplementationGuide>
